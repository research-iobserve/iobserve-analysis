package org.iobserve.common.record
	 
import kieker.common.record.flow.IEventRecord
import org.iobserve.common.record.GeoLocation
import org.iobserve.common.record.IEvent
	 
/** 
 * Marker interfaces for deployment and undeployment.	 
 */ 
@author "Reiner Jung" @since "0.0.2"
template IDeploymentChange
@author "Reiner Jung" @since "0.0.2"
template IDeployedEvent : IDeploymentChange, IEvent
@author "Reiner Jung" @since "0.0.2"
template IUndeployedEvent : IDeploymentChange, IEvent
	  
/**
 * Event records for PaaS servlet/WAR deployment/undeployment.
 */	 
@author "Reiner Jung" @since "0.0.2"
abstract event ServletDescriptor : IEvent { 
	string service
	string context
	string deploymentId
}

@author "Reiner Jung" @since "0.0.2"
event ServletDeployedEvent extends ServletDescriptor : IDeployedEvent
@author "Reiner Jung" @since "0.0.2"
event ServletUndeployedEvent extends ServletDescriptor : IUndeployedEvent

/**
 * Event records for PaaS servlet/WAR deployment/undeployment.	 
 */	 
@author "Reiner Jung" @since "0.0.2"
abstract event EJBDescriptor : IEvent { 
	string service
	string context
	string deploymentId
}

@author "Reiner Jung" @since "0.0.2"
event EJBDeployedEvent extends EJBDescriptor : IDeployedEvent
@author "Reiner Jung" @since "0.0.2"
event EJBUndeployedEvent extends EJBDescriptor : IUndeployedEvent

model Deployment IDeployedEvent, IUndeployedEvent
sub Privacy Deployment : GeoLocation


